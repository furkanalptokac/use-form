{
  "compilerOptions": {
    "target": "es2020",
    "module": "ESNext",
    "lib": ["es6", "DOM"],
    "allowJs": false,
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "Bundler",
    "jsx": "react-jsx"
  },
  "include": [
    "src",
  ],
  "exclude": ["dist", "node_modules"]
}
